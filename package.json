{
  "name": "handy-sales",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://127.0.0.1:8000/",
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@reduxjs/toolkit": "^1.3.6",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "moment": "^2.29.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-transition-group": "^4.4.1",
    "redux-localstorage-simple": "^2.2.0",
    "rewire": "^5.0.0",
    "typeface-roboto": "0.0.75",
    "uuid": "^8.1.0"
  },
  "scripts": {
    "start": "npm run watch:css && react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:bundle": "postcss src/assets/tailwind.css -o src/assets/main.css && node ./scripts/build-non-split.js env NODE_ENV=prod && purgecss --css build/static/css/main.css --content build/static/js/main.js --output build/static/css",
    "build:css": "postcss src/assets/tailwind.css -o src/assets/main.css",
    "watch:css": "postcss src/assets/tailwind.css -o src/assets/main.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.2.0",
    "@types/lodash": "^4.14.141",
    "autoprefixer": "^9.8.0",
    "express": "^4.17.1",
    "postcss-cli": "^7.1.1",
    "tailwindcss": "^1.4.6"
  }
}
